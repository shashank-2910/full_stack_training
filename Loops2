for of:
const b=[“1”,”2”,”3”]
for(let bb of b){
	console.log(bb);
}



While:

While(condition){
	Block of Code;
}

        let i=1;
        let n=10;
        while(i<=n){
            console.log(i);
            i++;
        }


let i=2;
        let n=8;

        while(i<=n){
            console.log(i+1);
            i=i+2;
        }


