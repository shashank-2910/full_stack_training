        // console.log(typeof(a))
        // console.log(typeof(b))
        // document.write(typeof(a),'<br>') 
        // document.write(typeof(b)) 

        let a=3.5;
        let b=3.5;
        let ex='10';
        console.log("hi+a+b");
        console.log('hi+a+b');
        console.log(`hi+${a+b}`);
        console.log(`${ex} Shashank`)
        document.write(`${ex} Shashank`,'<br>')
        console.log(`${ex}`,'Nani')
        console.log(`${ex}`)
        console.log(3+'10')

        let x='Hari'
        let y='Krishna'
        let age=30
        let greeting=`I am ${x+y}, my age is ${age} years`
        document.write(greeting,"<br>")

        let first_name='sai'
        let last_name='kumar'
        let result= `React students= ${first_name+last_name}`
        document.write(result,'<br>')



        
    </script>
</body>
